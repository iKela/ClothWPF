<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="ClothWPF.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <connectionStrings>
    <add name="EFContext"
         connectionString="data source=(LocalDb)\MSSQLLocalDB;initial catalog=ClothWPF;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  <userSettings>
    <ClothWPF.Properties.Settings>
      <setting name="DGNameVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGItemCodeVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGCountVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGLenghtVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGRetailVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGWholesaleVisibility" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGPurchaseDolPrice" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGPurchaseUahPrice" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DGCountryVisibility" serializeAs="String">
        <value>False</value>
      </setting>
    </ClothWPF.Properties.Settings>
  </userSettings>
</configuration>